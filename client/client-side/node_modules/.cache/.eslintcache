[],[],"/Users/nikitaizmailov/ml/wager_api/client/client-side/src/components_js/formUser.jsx",["27","28","29"],[],"import React, {useState, useEffect, useContext} from \"react\"\n\nfunction PlayerForm(props) {\n    \n    const [newWager, setNewWagers] = useState({\n        username: \"Nikita\",\n        game_type: \"Chess\",\n        website: \"\",\n        description: \"\",\n        wager_value: 0,\n        currency: \"USD\"\n    })\n    const [wagers, setWagers] = useState([\n        {\n            username: \"JokerrNikita\",\n            game_type: \"Chess\",\n            website: \"Chess.com\",\n            description: \"1 game, 3 minute + 1 second addon.\",\n            wager_value: 5,\n            currency: \"USD\"\n        },\n    ])\n\n    let isPlayerExist = false;\n    const playerData = [];\n\n    function handleChange(e) {\n        const {name, value} = e.target\n        setNewWagers(prevState => ({\n            ...prevState,\n            [name]: value\n        }));\n    }\n\n    function handleChangeAndFetch(e) {\n        const {name, value} = e.target\n        setNewWagers(prevState => ({\n            ...prevState,\n            [name]: value\n        }));\n\n        fetch(`http://127.0.0.1:8000/player_valid/${value}`).then((res) => {\n            return res.json();\n        }).then((data) => {\n            const prop = value;\n            if (prop === true) {\n                isPlayerExist = true\n            }\n            console.log(isPlayerExist);\n        })\n\n    }\n\n    function handleSubmit(e) {\n        e.preventDefault();\n\n        let updatedWagers = [...wagers, newWager];\n\n        setWagers(updatedWagers)\n    }\n\n    return (\n        <div className=\"app\">\n            <div className=\"form\">\n                <div className=\"form-title\">Let's Create A Wager?</div>\n                <div className=\"form-subtitle\">Input below details of your wager:</div>\n                <div className='input-container ic1'>\n                    <input\n                    id=\"username\"\n                    className=\"input\"\n                    type=\"text\"\n                    name=\"username\"\n                    placeholder=\" \"\n                    onChange={handleChangeAndFetch}\n                    />\n                    <div className=\"cut\"></div>\n                    {isPlayerExist === true && \n                        <div className=\"games-results\">\n                            <ul>\n                                <li>User: {newWager.username} exist!</li>\n                            </ul>\n                        </div>\n                    }\n                    <label for=\"username\" class=\"placeholder\">Your in-game username</label>\n                </div>\n                <div className='input-container ic2'>\n                    <input\n                    id=\"game_type\"\n                    className=\"input\"\n                    type=\"text\"\n                    name=\"game_type\"\n                    placeholder=\" \"\n                    onChange={handleChange}\n                    />\n                    <div className=\"cut\"></div>\n                    <label for=\"game_type\" className=\"placeholder\">Choose game genre</label>\n                </div>\n                <div className='input-container ic3'>\n                    <input\n                    id=\"website\"\n                    className=\"input\"\n                    type=\"text\"\n                    value={newWager.website}\n                    name=\"website\"\n                    placeholder=\" \"\n                    onChange={handleChange}\n                    />\n                    <div className=\"cut\"></div>\n                    <label for=\"website\" className=\"placeholder\">Website of the game</label>\n                </div>\n                <div className='input-container ic4'>\n                    <input\n                    id='description'\n                    className=\"input\"\n                    type=\"text\"\n                    name=\"description\"\n                    placeholder=\" \"\n                    onChange={handleChange}\n                    />\n                    <div className=\"cut\"></div>\n                    <label for=\"website\" className=\"placeholder\">Description of the wager</label>\n                </div>\n                <div className='input-container ic5'>\n                    <input\n                    id=\"wager_value\"\n                    className=\"input\"\n                    type=\"number\"\n                    name=\"wager_value\"\n                    placeholder=\" \"\n                    onChange={handleChange}\n                    />\n                    <div className=\"cut\"></div>\n                    <label for=\"wager_value\" className=\"placeholder\">Bet Amount</label>\n                </div>\n                <div className='input-container ic6'>\n                    <input\n                    id='currency'\n                    className=\"input\"\n                    type=\"text\"\n                    name=\"currency\"\n                    placeholder=\" \"\n                    onChange={handleChange}\n                    />\n                    <div className=\"cut\"></div>\n                    <label for=\"currency\" className=\"placeholder\">Currency</label>\n                </div>\n                <button className=\"submit\" onClick={handleSubmit}>\n                    Add a Wager\n                </button>\n            </div>\n            <div className=\"results\">\n                {wagers.map((wager, index) => (\n                    <div className=\"wagers-list\">\n                        <div className='wager'>\n                                <div>\n                                {wager.username}\n                                </div>\n                                <div>\n                                {wager.game_type}\n                                </div>\n                                <div>\n                                {wager.description}\n                                </div>\n                                <div>\n                                {wager.website}\n                                </div>\n                                <div>\n                                {wager.wager_value}\n                                </div>\n                                <div>\n                                {wager.currency}\n                                </div>\n                        </div>\n                    </div>\n                ))}\n            </div>\n        </div>\n    )\n\n}\n\nexport {PlayerForm}",{"ruleId":"30","severity":1,"message":"31","line":3,"column":21,"nodeType":"32","messageId":"33","endLine":3,"endColumn":28},{"ruleId":"30","severity":1,"message":"34","line":3,"column":30,"nodeType":"32","messageId":"33","endLine":3,"endColumn":43},{"ruleId":"30","severity":1,"message":"35","line":1,"column":26,"nodeType":"32","messageId":"33","endLine":1,"endColumn":35},{"ruleId":"30","severity":1,"message":"36","line":1,"column":37,"nodeType":"32","messageId":"33","endLine":1,"endColumn":47},{"ruleId":"30","severity":1,"message":"37","line":25,"column":11,"nodeType":"32","messageId":"33","endLine":25,"endColumn":21},"no-unused-vars","'Clicker' is defined but never used.","Identifier","unusedVar","'NameDisplayer' is defined but never used.","'useEffect' is defined but never used.","'useContext' is defined but never used.","'playerData' is assigned a value but never used."]